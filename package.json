{
  "name": "node-starter",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --outDir lib/",
    "clean": "rm -rf coverage lib",
    "typecheck": "tsc --noEmit",
    "jest": "jest --coverage --passWithNoTests",
    "test": "yarn typecheck && yarn jest"
  },
  "dependencies": {
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
